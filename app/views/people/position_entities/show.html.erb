<div class="container">
  <div class="card">
    <div class="card-block">
      <h4 class="card-title"><%= position_entity.name %></h4>
      <div class="card-text">
        <% position_entity.objectives.each do |objective| %>
          <%= link_to objective.name, edit_position_entity_objective_path(position_entity, objective) %>
          <% objective.key_results.each do |key_result| %>
            <div class="progress">
              <div class="progress-bar" role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
          <% end %>
        <% end %>
      </div>
    </div>
  </div>
</div>
