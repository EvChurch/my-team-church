<div class="nested-fields">
  <div class="row">
    <div class="col-sm-8">
      <%= f.input :name %>
    </div>
    <div class="col-sm-4">
      <%= f.input :weight %>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-4">
      <%= f.input :start_value %>
    </div>
    <div class="col-sm-4">
      <%= f.input :target_value %>
    </div>
    <div class="col-sm-4">
      <%= f.input :result_type %>
    </div>
  </div>
  <div class="actions text-right">
    <%= link_to_remove_association 'Remove Key Result', f, class: 'btn btn-danger' %>
  </div>
</div>
